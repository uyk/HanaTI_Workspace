
public class Exercise4_12 {
	public static void main(String[] args) {
		for(int j = 2; j <= 9; j +=3) {
			for(int i = 1; i <= 3; i++) {
				System.out.print(j + "*" + i + "=" + (j * i) + "\t");
				System.out.print((j+1) + "*" + i + "=" + (j=1)*i + "\t");
				System.out.print((j+2) * i + "\t");
				System.out.println();
			}
			System.out.println();
		}
	}
}
