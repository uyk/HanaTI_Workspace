
public class Rectangle extends Shape {
	private double width;
	private double height;
	
	
	public Rectangle() {
		this(0.0, 0.0, 0.0, 0.0);
		// TODO Auto-generated constructor stub
	}


	public Rectangle(double x, double y, double width, double height) {
		super(x, y);
		this.width = width;
		this.height = height;
	}
	
	
}
