/**
 * LIFO 구조의 스택 구현
 * 
 * @author 유예겸
 *
 */
public class Stack {
	private int[] array;
	
	/**
	 * 
	 * @param value
	 */
	public void push(int value) {
		
	}
	public int pop() {
		return 0;
	}
	//데이터의 개수 반환
	public int length() {
		return 0;
	}
	public static void main(String[] args) {
		//Stack stack = new Stack(100);
		Stack stack = new Stack();
		stack.push(5);
		stack.push(1);
		stack.push(9);
		//테스트를 위한 출력
		System.out.println(stack.pop());
		System.out.println(stack.pop());
		System.out.println(stack.pop());
		//예외처리 하지말고 심플한 기능만 구현.
		System.out.println(stack.length());
	}
}
